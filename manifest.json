{
  "manifest_version": 2,
  "name": "Graph Search Filter",
  "description": "Do more with FB!",
  "version": "1.0",
  "icons": {"48": "icon48.png", "72": "icon72.png", "128": "icon128.png"},
  "background": {
    "scripts":["frontend.js", "backend.js"],
    "persistence": false
  },
  "content_scripts": [{
    "matches": ["http://*.facebook.com/*", "https://*.facebook.com/*"],
    "js": ["./frontend.event.js", "./frontend.privacy.js", "./backend.js", "./jquery.min.js"],
    "run_at": "document_end"
  }]
}